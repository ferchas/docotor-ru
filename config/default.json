{
  "app": {
    "name": "Proyect Name",
    "cluster": false,
    "baseUrl": {
      "default": "localhost"
    },
    "port": 8081,
    "portS": 8443,
    "lang": [
      "es",
      "en"
    ]
  },
  "webpack": {
    "minimize-css": false,
    "uglify-js": false,
    "public-path": "./public/",
    "entryPoint": "./client/index.js",
    "layout": {
      "fileName": "index.html",
      "template": "./client/index.html"
    },
    "bundles": {
      "js": "bundle.[name].js",
      "css": "bundle.[name].css"
    }
  },
  "map": { 
    "key": ""
  }
}
